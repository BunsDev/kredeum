{
  "printWidth": 120,
  "singleQuote": false,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "semi": true,
  "plugins": ["./node_modules/prettier-plugin-solidity"],
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": false,
        "explicitTypes": "always"
      }
    }
  ]
}
