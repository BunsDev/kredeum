{
  "name": "kredeum",
  "version": "1.0.0",
  "description": "Kredeum NFTS Factory",
  "main": "index.js",
  "scripts": {
    "dapp": "http-server dapp/build",
    "clean:common": "cd common && npm run clean",
    "clean:hardhat": "cd hardhat && npm run clean",
    "clean:gulp": "cd gulp && npm run clean",
    "clean:rollup": "cd rollup && npm run clean",
    "clean:dapp": "rm -rf dapp/build",
    "clean:wordpress": "cd wordpress && npm run clean",
    "clean:thegraph": "cd thegraph && npm run clean",
    "build:common": "cd common && pnpm install",
    "build:hardhat": "cd hardhat && pnpm install && npm run build",
    "build:gulp": "cd gulp && pnpm install && npm run build",
    "build:rollup": "cd rollup && pnpm install && npm run build",
    "build:wordpress": "cd wordpress && pnpm install && npm run instal && npm run build",
    "build:thegraph": "cd thegraph && pnpm install && npm run build",
    "check:hardhat": "cd hardhat && npm run check",
    "check:rollup": "cd rollup && npm run check",
    "check:wordpress": "cd wordpress && npm run check",
    "clean": "npm-run-all clean:*",
    "build": "npm-run-all build:*",
    "check": "npm-run-all check:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kredeum/kredeum.git"
  },
