{
  "favorites.resources": [
    "solidity",
    "wordpress/readme.txt_handlebars",
    "package.json",
    ".vscode/settings.json",
    "hardhat.config.js",
    "solidity/deploy/OpenNFTS.js",
    "config/networks.json",
    "solidity/contracts/OpenNFTs.sol",
    "hardhat.config.ts",
    "solidity/contracts/CloneFactory.sol",
    "solidity/tests/2-clone-factory.ts"
  ],
  "files.exclude": {
    "old": true
  },
  "mochaExplorer.exit": true,
  "mochaExplorer.files": "solidity/tests/*.*ts",
  "mochaExplorer.fullTrace": true,
  "mochaExplorer.logpanel": true,
  // "mochaExplorer.nodeArgv": ["--experimental-json-modules"],
  "mochaExplorer.nodePath": "/usr/local/bin/node",
  "mochaExplorer.optsFile": "",
  "mochaExplorer.retries": 1,
  "mochaExplorer.timeout": 20000,
  "mochaExplorer.jobs": 6,
  "phpsab.debug": true,
  "phpsab.executablePathCBF": "wordpress/vendor/bin/phpcbf",
  "phpsab.executablePathCS": "wordpress/vendor/bin/phpcs",
  "phpsab.fixerArguments": ["--extensions=php"],
  "phpsab.snifferArguments": ["-n", "--ignore=lib", "--extensions=php"],
  "phpsab.snifferMode": "onType",
  "phpsab.snifferShowSources": true,
  "phpsab.standard": "WordPress",
  "solidity.compileUsingRemoteVersion": "v0.8.4+commit.c7e474f2",
  "testExplorer.codeLens": true,
  "workbench.colorTheme": "Noctis Viola",
  "favorites.sortOrder": "ASC",
  "liveServer.settings.port": 5501,
  "liveServer.settings.multiRootWorkspaceName": "kredeum",
  "liveServer.settings.root": "/app"
}
